{
  "name": "level-airplanedb",
  "description": "leveldb airplane mode with manual replication",
  "keywords": [
    "leveldb",
    "leveldown",
    "levelup",
    "browserify",
    "offline first",
    "mobile",
    "replication",
    "airplane mode"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/level-airplanedb"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "main": "index.js",
  "browser": "browser.js",
  "browserify": {
    "transform": [
      "bulkify"
    ]
  },
  "version": "0.0.6",
  "scripts": {
    "test": "testling | faucet"
  },
  "testling": {
    "server": "testserver.js",
    "files": "test/*.js",
    "browsers": [
      "ie/10..latest",
      "firefox/18..latest",
      "firefox/nightly",
      "chrome/25..latest",
      "chrome/canary",
      "opera/15..latest",
      "opera/next",
      "safari/6..latest",
      "iphone/6..latest",
      "ipad/6..latest",
      "android-browser/4.2..latest"
    ]
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.26"
  },
  "dependencies": {
    "IndexedDBShim": "git://github.com/jameskyburz/IndexedDBShim.git#master",
    "json-stable-stringify": "^0.1.2",
    "level-delete-range": "^0.1.0",
    "level-sublevel": "^5.2.0",
    "map-stream": "~0.1.0",
    "monot": "^0.1.2",
    "shoe": "0.0.15"
  },
  "devDependencies": {
    "browserify": "^3.44.2",
    "bulkify": "^1.0.0",
    "faucet": "0.0.1",
    "function-bind": "^1.0.0",
    "level-js": "^2.1.3",
    "leveldown-prebuilt": "^0.10.2",
    "levelup": "^0.18.3",
    "multilevel": "^6.0.0",
    "rimraf": "^2.2.6",
    "shoe": "0.0.15",
    "tap-browser-color": "^0.1.2",
    "tape": "^2.12.3",
    "testling": "^1.6.1"
  }
