{
  "name": "level-airplanedb",
  "description": "leveldb airplane mode with manual replication",
  "keywords": [
    "leveldb",
    "leveldown",
    "levelup",
    "browserify",
    "offline first",
    "mobile",
    "replication",
    "airplane mode"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jameskyburz/level-airplanedb"
  },
  "author": {
    "name": "James Kyburz",
    "email": "james.kyburz@gmail.com"
  },
  "main": "index.js",
  "browser": "browser.js",
  "browserify": {
    "transform": [
      "bulkify"
    ]
  },
  "version": "1.0.8",
  "scripts": {
    "test": "standard && zuul test/*.js --phantom | faucet",
    "test-local": "zuul test/*.js --local --open",
    "test-browsers": "remove-saucelabs-jobs-by-build && zuul test/*.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.26"
  },
  "dependencies": {
    "indexeddbshim": "2.2.1",
    "json-stable-stringify": "1.0.1",
    "level-delete-range": "0.1.0",
    "level-sublevel": "5.2.3",
    "map-stream": "0.0.6",
    "monot": "0.1.2"
  },
  "devDependencies": {
    "bulkify": "https://github.com/JamesKyburz/bulkify.git#path-transform-fix",
    "engine.io-stream": "0.4.3",
    "faucet": "0.0.1",
    "function-bind": "1.0.2",
    "hyperquest": "1.2.0",
    "level-js": "2.2.3",
    "level-proxy": "1.1.2",
    "leveldown": "1.4.4",
    "levelup": "0.19.0",
    "multilevel": "6.2.1",
    "rimraf": "2.5.1",
    "remove-saucelabs-jobs-by-build": "1.0.6",
    "standard": "6.0.4",
    "tape": "4.4.0",
    "zuul": "3.9.0"
  }
}
